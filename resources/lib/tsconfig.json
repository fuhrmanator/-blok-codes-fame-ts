{
    "compilerOptions": {
        "declaration": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "lib": [
            "esnext",
            "es2019",
            "dom"
        ],
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noLib": false,
        "noUnusedLocals": true,
        "outDir": "dist",
        "removeComments": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": false,
        "target": "es2019",
        "typeRoots": [
            "./node_modules/@types",
            "./types"
        ]
    },
    "exclude": [
        "dist",
        "node_modules",
    ],
    "include": [
        "Famix-Traits/**/*",
        "FamixReplication/**/*",
        "FamixTypeScript/**/*",
        "helpers/**/*",
        "Moose/**/*",
        "Moose-Query/**/*",
        "Tagging/**/*",
        "BaseElement.ts",
        "Decorators.ts",
        "FamixBaseElement.ts",
        "FamixJSONExporter.ts",
        "FamixJSONImporter.ts",
        "SetWithOpposite.ts"
    ]
}
